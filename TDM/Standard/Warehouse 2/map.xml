<?xml version="1.0"?>
<map proto="1.3.2">
<name>Warehouse 2</name>
<version>1.1.0</version>
<objective>Be the team with the least amount of deaths when the time runs out.</objective>
<authors>
    <author uuid="2958ea4c-3501-404e-88cc-12e8632f68cc"/> <!-- King_Coolwhip -->
</authors>
<teams>
    <team color="blue" max="16">Blue</team>
    <team color="yellow" max="16">Yellow</team>
</teams>
<kits>
    <kit name="spawn">
        <item slot="0" unbreakable="true">stone sword</item>
        <item slot="1" unbreakable="true" enchantment="arrow infinite:1">bow</item>
        <item slot="28" amount="1">arrow</item>
        <item slot="2" amount="64">golden carrot</item>
        <item slot="3" amount="1">golden apple</item>
        <leggings unbreakable="true">chainmail leggings</leggings>
        <potion duration="4">heal</potion>
        <potion duration="4">damage resistance</potion>
    </kit>
    <kit name="blue" parents="spawn">
        <helmet unbreakable="true" color="334CB2">leather helmet</helmet>
        <chestplate unbreakable="true" color="334CB2">leather chestplate</chestplate>
        <boots unbreakable="true" color="334CB2">leather boots</boots>
    </kit>
    <kit name="yellow" parents="spawn">
        <helmet unbreakable="true" color="E5E533">leather helmet</helmet>
        <chestplate unbreakable="true" color="E5E533">leather chestplate</chestplate>
        <boots unbreakable="true" color="E5E533">leather boots</boots>
    </kit>
</kits>
<spawns>
    <spawn team="blue" kit="blue" yaw="180">
        <cylinder base="0,5,46" radius="2" height="1" />
    </spawn>
    <spawn team="yellow" kit="yellow" yaw="0">
        <cylinder base="0,5,-46" radius="2" height="1" />
    </spawn>
    <default yaw="179">
        <cylinder base="30,23,28" radius="1" height="0" />
    </default>
</spawns>
<regions>
    <apply block="deny-all">
        <rectangle min="-oo,-oo" max="oo,oo" />
    </apply>
</regions>
<score>
    <time>10m</time>
</score>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>chainmail leggings</item>
    <item>arrow</item>
    <item>golden carrot</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>iron leggings</item>
    <item>leather boots</item>
    <item>golden apple</item>
</itemremove>
<killreward>
    <item>golden apple</item>
</killreward>
<timelock>on</timelock>
</map>
