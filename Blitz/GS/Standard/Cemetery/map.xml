</rules>
<teams>
    <team color="dark purple" max="5">Purple</team>
    <team color="dark green" max="5">Green</team>
</teams>
<spawns>
    <spawn team="purple" yaw="180">
        <point>-16,50,-14</point>
    </spawn>
    <spawn team="green" yaw="0">
        <point>-16,50,13</point>
    </spawn>
    <default yaw="-90">
        <point>-0.5,67,-0.5</point>
    </default>
</spawns>
