<?xml version="1.0"?>
<map proto="1.3.0">
<name>Splinter</name>
<version>1.2.2</version>
<objective>Kill the enemy's squadron!</objective>
<gamemode>gs</gamemode>
<include src="gs.xml"/>
<authors>
    <author uuid="459fd919-0d58-401a-9601-6cab1eb721ab"/> <!-- Reshif -->
</authors>
<teams>
    <team color="blue" max="8">Blue</team>
    <team color="dark red" max="8">Red</team>
</teams>
<spawns>
    <spawn team="red" yaw="135">
        <cylinder base="32.5,7,32.5" radius="3" height="0"/>
    </spawn>
    <spawn team="blue" yaw="315">
        <cylinder base="-31.5,7,-31.5" radius="3" height="0"/>
    </spawn>
    <default>
        <point yaw="0">
            <cylinder base="0.5,21,0.5" radius="2" height="0"/>
        </point>
        <point yaw="90">
            <cylinder base="0.5,21,0.5" radius="2" height="0"/>
        </point>
        <point yaw="180">
            <cylinder base="0.5,21,0.5" radius="2" height="0"/>
        </point>
        <point yaw="270">
            <cylinder base="0.5,21,0.5" radius="2" height="0"/>
        </point>
    </default>
</spawns>
</map>
