<map proto="1.3.2">
<name>War Wars: SolitudeMC</name>
<version>1.0.0</version>
<objective>Leak lava from the enemy's core into the void.</objective>
<authors>
    <author>matic0basle</author>
</authors>
<contributors>
    <contributor contribution="Spawner Help">TheSecret8</contributor>
</contributors>
<mobs>
    <filter>
        <spawn>spawner</spawn>
    </filter>
</mobs>
<regions>
    <negative name="outside">
        <rectangle name="playable" min="17,-17" max="-16,16"/>
    </negative>
</regions>
<blitz>
    <title>War Wars</title>
    <time>3m</time>
    <lives>1</lives>
</blitz>
<teams>
    <team color="blue" max="5">Blue</team>
    <team color="dark red" max="5">Red</team>
</teams>
<spawns>
    <spawn team="blue" yaw="90">7.5,21,0.5</spawn>
    <spawn team="red" yaw="-90">-6.5,21,0.5</spawn>
    <default yaw="0">0.5,29,-11.5</default>
</spawns>
<cores material="obsidian" leak="3" mode-changes="true">
    <core team="blue">
        <cuboid min="12,31,-1" max="9,27,2"/>
    </core>
    <core team="red">
        <cuboid min="-8,27,-1" max="-11,31,2"/>
    </core>
</cores>
<modes>
    <mode after="2m30s" material="tnt" name="EASY MODE ACTIVATED"/>
</modes>
<portals>
    <portal y="-15000">
        <region name="outside"/>
    </portal>
</portals>
</map>
